{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "resources/js/*"
      ],
      "@components/*": [
        "resources/js/Components/*"
      ],
      "@pages/*": [
        "resources/js/Pages/*"
      ],
      "@layouts/*": [
        "resources/js/Layouts/*"
      ],
      "@store/*": [
        "resources/js/stores/*"
      ],
      "@composables/*": [
        "resources/js/Composables/*"
      ],
      "@utils/*": [
        "resources/js/utils/*"
      ],
      "@services/*": [
        "resources/js/services/*"
      ],
      "@definitions": [
        "resources/js/definitions"
      ],
      "@definitions/*": [
        "resources/js/definitions/*"
      ],
    }
  },
  "include": [
    "resources/js/**/*",
  ],
  "exclude": [
    "node_modules"
  ]
}
